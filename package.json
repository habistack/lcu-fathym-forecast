{
	"name": "@habistack/lcu-fathym-forecast",
	"version": "0.0.26",
	"scripts": {
		"build:common": "ng build common",
		"build:demo": "ng build demo --prod",
		"build:lcu": "ng build lcu --prod --single-bundle && npm run pack",
		"demo": "npm run build:common && npm run start:demo",
		"demo:lcu": "npm run build:common && npm run build:lcu && npm run start:demo",
		"deploy": "npm version patch && npm run deploy:all",
		"deploy:all": "npm run deploy:common && npm run deploy:lcu && npm run deploy:demo",
		"deploy:common": "npm version patch --prefix projects/common && npm run build:common && npm publish ./dist/common --access public",
		"deploy:demo": "npm version patch --prefix projects/demo && npm run build:demo && npm publish ./dist/demo --access public",
		"deploy:lcu": "npm version patch --prefix projects/lcu && npm run build:lcu && npm publish ./dist/lcu --access public",
		"ng": "ng",
		"pack": "npm run pack:lcu",
		"pack:join": "concat-glob-cli -f \"dist/lcu/wc/lcu.*.js\" -o dist/lcu/wc/lcu-fathym-forecast.lcu.js",
		"pack:lcu": "rimraf dist/lcu/wc && mkdirp dist/lcu/wc && npm run pack:main && npm run pack:pollyfills && npm run pack:join",
		"pack:main": "concat-glob-cli -f \"dist/lcu/main-es2015.*.js\" -o dist/lcu/wc/lcu.startup.js",
		"pack:pollyfills": "concat-glob-cli -f \"dist/lcu/scripts.*.js\" -o dist/lcu/wc/lcu.pollyfills.js",
		"start": "ng serve demo --port=4210",
		"build": "npm run build:common && npm run build:lcu",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"npx-build-plus:copy-assets": "node projects/lcu/copy-wc-polyfill.js"
	},
	"private": false,
	"dependencies": {
		"@angular/animations": "^10.1.4",
		"@angular/cdk": "^10.2.3",
		"@angular/common": "^10.1.4",
		"@angular/compiler": "^10.1.4",
		"@angular/core": "^10.1.4",
		"@angular/elements": "^10.1.4",
		"@angular/flex-layout": "^10.0.0-beta.32",
		"@angular/forms": "^10.1.4",
		"@angular/material": "^10.2.3",
		"@angular/platform-browser": "^10.1.4",
		"@angular/platform-browser-dynamic": "^10.1.4",
		"@angular/router": "^10.1.4",
		"@aspnet/signalr": "^1.1.4",
		"@beyerleinf/ngx-dnd": "8.0.3",
		"@lcu/common": "^1.45.359",
		"@lowcodeunit/data-grid": "^1.2.22",
		"@lowcodeunit/lazy-element": "^1.3.4",
		"@lowcodeunit/lcu-charts-common": "^1.9.44",
		"@lowcodeunit/lcu-maps-common": "^1.8.51",
		"@lowcodeunit/lcu-select-common": "^1.2.28",
		"@types/d3": "^5.7.2",
		"@webcomponents/custom-elements": "^1.4.2",
		"@webcomponents/webcomponentsjs": "^2.4.4",
		"core-js": "^3.6.5",
		"d3": "^6.1.1",
		"hammerjs": "^2.0.8",
		"ng2-file-upload": "^1.4.0",
		"ngx-build-plus": "^10.1.1",
		"ngx-toastr": "13.0.1",
		"rxjs": "~6.6.3",
		"terser": "^5.3.3",
		"zone.js": "~0.11.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.1001.4",
		"@angular-devkit/build-ng-packagr": "^0.1001.4",
		"@angular/cli": "^10.1.4",
		"@angular/compiler-cli": "^10.1.4",
		"@angular/language-service": "^10.1.4",
		"@lcu/cli": "^1.7.97",
		"@lowcodeunit-devkit/ng-lcu": "^1.23.147",
		"@types/jasmine": "~3.5.14",
		"@types/jasminewd2": "~2.0.8",
		"@types/node": "^14.11.2",
		"codelyzer": "^6.0.1",
		"concat-glob-cli": "^0.1.0",
		"domelementtype": "^2.0.2",
		"jasmine-core": "~3.6.0",
		"jasmine-spec-reporter": "~6.0.0",
		"karma": "~5.2.3",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~3.0.3",
		"karma-jasmine": "~4.0.1",
		"karma-jasmine-html-reporter": "^1.5.4",
		"mkdirp": "^1.0.4",
		"ng-packagr": "^10.1.2",
		"protractor": "~7.0.0",
		"ts-node": "~9.0.0",
		"tsickle": "0.39.1",
		"tslib": "^2.0.1",
		"tslint": "~6.1.3",
		"typescript": "4.0.3"
	}
}
