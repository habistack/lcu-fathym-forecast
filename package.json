{
  "name": "@habistack/lcu-fathym-forecast",
  "version": "0.0.26",
  "scripts": {
    "build:common": "ng build common",
    "build:demo": "ng build demo --prod",
    "build:lcu": "ng build lcu --prod --single-bundle && npm run pack",
    "demo": "npm run build:common && npm run start:demo",
    "demo:lcu": "npm run build:common && npm run build:lcu && npm run start:demo",
    "deploy": "npm version patch && npm run deploy:all",
    "deploy:all": "npm run deploy:common && npm run deploy:lcu && npm run deploy:demo",
    "deploy:common": "npm version patch --prefix projects/common && npm run build:common && npm publish ./dist/common --access public",
    "deploy:demo": "npm version patch --prefix projects/demo && npm run build:demo && npm publish ./dist/demo --access public",
    "deploy:lcu": "npm version patch --prefix projects/lcu && npm run build:lcu && npm publish ./dist/lcu --access public",
    "ng": "ng",
    "pack": "npm run pack:lcu",
    "pack:join": "concat-glob-cli -f \"dist/lcu/wc/lcu.*.js\" -o dist/lcu/wc/lcu-fathym-forecast.lcu.js",
    "pack:lcu": "rimraf dist/lcu/wc && mkdirp dist/lcu/wc && npm run pack:main && npm run pack:pollyfills && npm run pack:join",
    "pack:main": "concat-glob-cli -f \"dist/lcu/main-es2015.*.js\" -o dist/lcu/wc/lcu.startup.js",
    "pack:pollyfills": "concat-glob-cli -f \"dist/lcu/scripts.*.js\" -o dist/lcu/wc/lcu.pollyfills.js",
    "start": "ng serve demo --port=4210",
    "build": "npm run build:common && npm run build:lcu",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "npx-build-plus:copy-assets": "node projects/lcu/copy-wc-polyfill.js"
  },
  "private": false,
  "dependencies": {
    "@acaisoft/angular-azure-maps": "^1.0.0",
    "@angular/animations": "~9.0.7",
    "@angular/cdk": "^9.1.3",
    "@angular/common": "~9.0.7",
    "@angular/compiler": "~9.0.7",
    "@angular/core": "~9.0.7",
    "@angular/elements": "^9.0.7",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~9.0.7",
    "@angular/material": "^9.1.3",
    "@angular/platform-browser": "~9.0.7",
    "@angular/platform-browser-dynamic": "~9.0.7",
    "@angular/router": "~9.0.7",
    "@aspnet/signalr": "^1.1.4",
    "@beyerleinf/ngx-dnd": "^6.0.0-preview3",
    "@lcu/common": "^1.39.159",
    "@lowcodeunit/data-grid": "1.1.6-angular-9",
    "@lowcodeunit/lcu-charts-common": "^1.7.6",
    "@lowcodeunit/lcu-select-common": "^1.1.13-angular-9",
    "@types/d3": "^5.7.2",
    "@webcomponents/custom-elements": "^1.4.1",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "core-js": "^2.6.1",
    "d3": "^5.16.0",
    "hammerjs": "^2.0.8",
    "ng2-file-upload": "^1.3.0",
    "ngx-build-plus": "^8.1.3",
    "ngx-toastr": "^10.1.0",
    "rxjs": "~6.5.4",
    "terser": "^4.6.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.7",
    "@angular-devkit/build-ng-packagr": "~0.900.7",
    "@angular/cli": "~9.0.7",
    "@angular/compiler-cli": "~9.0.7",
    "@angular/language-service": "~9.0.7",
    "@lcu/cli": "^1.2.9",
    "@lowcodeunit-devkit/ng-lcu": "^1.5.26",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concat-glob-cli": "^0.1.0",
    "domelementtype": "^2.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "mkdirp": "^1.0.3",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  }
}
