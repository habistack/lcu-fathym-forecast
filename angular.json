    "wc-demo-forge": {
      "root": "projects/wc-demo-forge/",
      "sourceRoot": "projects/wc-demo-forge/src",
      "projectType": "application",
      "prefix": "lib",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "C:\\Fathym\\Git\\Apps\\Forge\\Fathym.Forge.Web\\wwwroot\\marketing",
            "index": "projects/wc-demo-forge/src/index.html",
            "main": "projects/wc-demo-forge/src/main.ts",
            "polyfills": "projects/wc-demo-forge/src/polyfills.ts",
            "tsConfig": "projects/wc-demo-forge/tsconfig.app.json",
            "assets": [
              "projects/wc-demo-forge/src/favicon.ico",
              "projects/wc-demo-forge/src/assets",
              {
                "glob": "package.json",
                "input": "./",
                "output": "/"
              }
            ],
            "styles": [
              "projects/wc-demo-forge/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/wc-demo-forge/src/environments/environment.ts",
                  "with": "projects/wc-demo-forge/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "wc-demo-forge:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "wc-demo-forge:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wc-demo-forge:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/wc-demo-forge/src/test.ts",
            "polyfills": "projects/wc-demo-forge/src/polyfills.ts",
            "tsConfig": "projects/wc-demo-forge/tsconfig.spec.json",
            "karmaConfig": "projects/wc-demo-forge/karma.conf.js",
            "styles": [
              "projects/wc-demo-forge/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "projects/wc-demo-forge/src/favicon.ico",
              "projects/wc-demo-forge/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/wc-demo-forge/tsconfig.app.json",
              "projects/wc-demo-forge/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "wc-demo-forge-e2e": {
      "root": "projects/wc-demo-forge-e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/wc-demo-forge-e2e/protractor.conf.js",
            "devServerTarget": "wc-demo-forge:serve"
          },
          "configurations": {
